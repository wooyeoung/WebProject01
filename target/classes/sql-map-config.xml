<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  
  	<!-- Alias(별명) 설정 -->
  	<typeAliases> 
  		<typeAlias alias="member" type="com.vegan.admin.member.repository.MemberVO" />
  		<typeAlias alias="zipcode" type="com.vegan.admin.member.repository.ZipcodeVO" />
  		<typeAlias alias="community" type="com.vegan.user.community.repository.CommunityVO" />
  		<typeAlias alias="qaa" type="com.vegan.user.qaa.biz.QaaBoardVO"/>
		<typeAlias alias="adminqaa" type="com.vegan.admin.qaa.biz.AdminQaaBoardVO"/>
  		<typeAlias alias="adminfaq" type="com.vegan.admin.faq.biz.AdminFaqBoardVO"/>
		<typeAlias alias="faq" type="com.vegan.user.faq.biz.faqBoardVO"/>
  		<typeAlias alias="basket" type="com.vegan.user.basket.dao.BasketVO"/>		
		<typeAlias alias="order" type="com.vegan.user.order.biz.OrderBoardVO"/>
		<typeAlias alias="goods" type="com.vegan.admin.goods.db.GoodsVO"/>
		<typeAlias alias="review" type="com.vegan.user.goods.review.db.ReviewVO"/>
		<typeAlias alias="recipe" type="com.vegan.admin.recipe.db.RecipeVO"/>
  	
  	</typeAliases>
  	
  	<!-- Sql Mapper 설정 -->
  	<mappers>
  		<mapper resource="mappings/member-mapping.xml"/>
  		<mapper resource="mappings/zipcode-mapping.xml"/>
  		<mapper resource="mappings/community-mapping.xml"/>
  		<mapper resource="mappings/qaa-mapping.xml"/>
		<mapper resource="mappings/adminqaa-mapping.xml"/>
		<mapper resource="mappings/adminFaq-mapping.xml"/>
		<mapper resource="mappings/faq-mapping.xml"/>
		<mapper resource="mappings/basket-mapping.xml"/>
		<mapper resource="mappings/order-mapping.xml"/>
		<mapper resource="mappings/goods-mapping.xml"/>
		<mapper resource="mappings/review-mapping.xml"/>
		<mapper resource="mappings/recipe-mapping.xml"/>
  	</mappers>
  
</configuration>